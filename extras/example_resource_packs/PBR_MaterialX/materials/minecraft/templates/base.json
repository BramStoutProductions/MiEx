                    }
                }
            },
            "INTERPOLATE_NORMAL": {
                "type": "ND_mix_float3",
                "attributes": {
                    "outputs:out": {
                        "type": "float3"
                    },
                    "inputs:bg": {
                        "type": "float3",
                        "connection": "FILE_NORMAL.outputs:out"
                    },
                    "inputs:fg": {
                        "type": "float3",
                        "connection": "FILE_NORMAL2.outputs:out"
                    },
                    "inputs:mix": {
                        "type": "float",
                        "expression": "${@texture@.interpFactor}"
                    }
                }
            },
            "NORMAL_MAP_NORMAL": {
                "attributes": {
                    "inputs:in": {
                        "type": "float3",
                        "connection": "INTERPOLATE_NORMAL.outputs:out"
                    }
                }
            }
        },



