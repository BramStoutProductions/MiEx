                    "outputs:out": {
                        "type": "float2"
                    },
                    "inputs:in": {
                        "type": "float2",
                        "connection": "${FILE_NORMAL.inputs:texcoord}"
                    },
                    "inputs:scale": {
                        "type": "float2",
                        "expression": "${@texture@.frameScale}"
                    },
                    "inputs:translation": {
                        "type": "float2",
                        "expression": "${@texture@.frameId(offset=1)}"
                    }
                }
            },
            "FILE_NORMAL": {
                "attributes": {
                    "inputs:texcoord": {
                        "connection": "UV_NORMAL_ANIMATED.outputs:out"
                    }
                }
            },
            "FILE_NORMAL2": {
                "type": "ND_image_vector3",
                "attributes": {
                    "outputs:out": {
                        "type": "float3"
                    },
                    "inputs:file": {
                        "type": "asset",
                        "value": "@texture@_normal"
