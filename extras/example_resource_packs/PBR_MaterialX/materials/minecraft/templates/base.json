{
    "priority": 1,
    "selection": [ "*" ],
    "include": [ "base" ],
    "network": {
        "@texture@": {
            "MAT": {
                "attributes": {
                    "inputs:specular": {
                        "type": "float",
                        "value": 0.5
                    },
                    "inputs:specular_roughness": {
                        "type": "float",
                        "value": 0.5
                    }
                }
            }
        },

        "@texture@_normal": {
            "MAT": {
                "attributes": {
                    "inputs:normal": {
                        "type": "float3",
                        "connection": "NORMAL_MAP_NORMAL.outputs:out"
                    }
                }
            },
            "FILE_NORMAL": {
                "type": "ND_image_vector3",
                "attributes": {
                    "outputs:out": {
                        "type": "float3"
                    },
                    "inputs:file": {
                        "type": "asset",
                        "value": "@texture@_normal"
                    },
                    "inputs:filtertype": {
                        "type": "string",
                        "value": "closest"
                    },
                    "inputs:texcoord": {
                        "type": "float2",
                        "connection": "UV.outputs:out"
                    }
                }
            },
            "NORMAL_NORMAL": {
                "type": "ND_normal_vector3",
                "attributes": {
                    "outputs:out": {
                        "type": "float3"
                    }
                }
            },
            "POSITION_NORMAL":{
                "type": "ND_position_vector3",
                "attributes": {
                    "outputs:out": {
                        "type": "float3"
                    }
                }
            },
            "TANGENT_NORMAL": {
                "type": "MayaND_texcoordtangents_vector3",
                "attributes": {
                    "outputs:out": {
                        "type": "float3"
                    },
                    "inputs:position": {
                        "type": "float3",
                        "connection": "POSITION_NORMAL.outputs:out"
                    },
                    "inputs:normal": {
                        "type": "float3",
                        "connection": "NORMAL_NORMAL.outputs:out"
                    },
                    "inputs:texcoord": {
                        "type": "float2",
                        "connection": "UV.outputs:out"
                    }
                }
            },
            "NORMAL_MAP_NORMAL": {
                "type": "ND_normalmap",
                "attributes": {
                    "outputs:out": {
                        "type": "float3"
                    },
                    "inputs:in": {
                        "type": "float3",
                        "connection": "FILE_NORMAL.outputs:out"
                    },
                    "inputs:normal": {
                        "type": "float3",
                        "connection": "NORMAL_NORMAL.outputs:out"
                    },
                    "inputs:tangent": {
                        "type": "float3",
                        "connection": "TANGENT_NORMAL.outputs:out"
                    }
                }
            }
        },
        "@texture@_normal&&@texture@.animated": {
            "UV_NORMAL_ANIMATED": {
                "type": "ND_UsdTransform2d",
                "attributes": {
                    "outputs:out": {
                        "type": "float2"
                    },
                    "inputs:in": {
                        "type": "float2",
                        "connection": "${FILE_NORMAL.inputs:texcoord}"
                    },
                    "inputs:scale": {
                        "type": "float2",
                        "expression": "${@texture@.frameScale}"
                    },
                    "inputs:translation": {
                        "type": "float2",
                        "expression": "${@texture@.frameId}"
                    }
                }
            },
            "FILE_NORMAL": {
                "attributes": {
                    "inputs:texcoord": {
                        "connection": "UV_NORMAL_ANIMATED.outputs:out"
                    }
                }
            }
        },
        "@texture@_normal&&@texture@.interpolated": {
            "UV_NORMAL_ANIMATED": {
                "type": "ND_UsdTransform2d",
                "attributes": {
                    "outputs:out": {
                        "type": "float2"
                    },
                    "inputs:in": {
                        "type": "float2",
                        "connection": "${FILE_NORMAL.inputs:texcoord}"
