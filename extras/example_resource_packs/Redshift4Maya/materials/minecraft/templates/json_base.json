{
    "priority": 0,
    "selection": [ "*" ],
    "shadingGroup": {
        "surface": "PLACEHOLDER.outputs:surface",
        "json:surfaceShader": "MAT.outColor"
    },
    "include": [ "json_base" ],
    "network": {
        "@texture@": {
            "PLACEHOLDER": {
                "type": "UsdPreviewSurface",
                "attributes": {
                    "outputs:surface": {
                        "type": "token"
                    }
                }
            },
            "MAT": {
                "type": "JSON:RedshiftMaterial",
                "attributes": {
                    "outColor": {
                        "type": "token"
                    },
                    "diffuse_color": {
                        "type": "float3",
                        "connection": "FILE.outColor"
                    },
                    "refl_weight": {
                        "type": "float",
                        "value": 0.0
                    }
                }
            },
            "FILE": {
                "type": "JSON:file",
                "attributes": {
                    "outColor": {
                        "type": "float3"
                    },
                    "fileTextureName": {
                        "type": "asset",
                        "value": "@texture@"
                    },
                    "filterType": {
                        "type": "int",
                        "value": 0
                    },
                    "rsFilterEnable": {
                        "type": "int",
                        "value": 0
                    },
                    "coverage": {
                        "type": "float2",
                        "connection": "UV.coverage"
                    },
                    "mirrorU": {
                        "type": "int",
                        "connection": "UV.mirrorU"
                    },
                    "mirrorV": {
                        "type": "int",
                        "connection": "UV.mirrorV"
                    },
                    "noiseUV": {
                        "type": "float2",
                        "connection": "UV.noiseUV"
                    },
                    "offset": {
                        "type": "float2",
                        "connection": "UV.offset"
                    },
                    "repeatUV": {
                        "type": "float2",
                        "connection": "UV.repeatUV"
                    },
                    "rotateFrame": {
                        "type": "float",
                        "connection": "UV.rotateFrame"
                    },
                    "rotateUV": {
                        "type": "float",
                        "connection": "UV.rotateUV"
                    },
                    "stagger": {
                        "type": "int",
                        "connection": "UV.stagger"
                    },
                    "translateFrame": {
                        "type": "float2",
                        "connection": "UV.translateFrame"
                    },
                    "uvCoord": {
                        "type": "float2",
                        "connection": "UV.outUV"
                    },
                    "uvFilterSize": {
                        "type": "float2",
                        "connection": "UV.outUvFilterSize"
                    },
                    "vertexCameraOne": {
                        "type": "float3",
                        "connection": "UV.vertexCameraOne"
                    },
                    "vertexUvOne": {
                        "type": "float2",
                        "connection": "UV.vertexUvOne"
                    },
                    "vertexUvTwo": {
                        "type": "float2",
                        "connection": "UV.vertexUvTwo"
                    },
                    "vertexUvThree": {
                        "type": "float2",
                        "connection": "UV.vertexUvThree"
                    },
                    "wrapU": {
                        "type": "int",
                        "connection": "UV.wrapU"
                    },
                    "wrapV": {
                        "type": "int",
                        "connection": "UV.wrapV"
                    }
                }
            },
            "UV": {
                "type": "JSON:place2dTexture",
                "attributes": {
                    "coverage": {
                        "type": "float2"
                    },
                    "mirrorU": {
                        "type": "int"
                    },
                    "mirrorV": {
                        "type": "int"
                    },
                    "noiseUV": {
                        "type": "float2"
                    },
                    "offset": {
                        "type": "float2"
                    },
                    "repeatUV": {
                        "type": "float2"
                    },
                    "rotateFrame": {
                        "type": "float"
                    },
                    "rotateUV": {
                        "type": "float"
                    },
                    "stagger": {
                        "type": "int"
                    },
                    "translateFrame": {
                        "type": "float2"
                    },
                    "uvCoord": {
                        "type": "float2"
                    },
                    "uvFilterSize": {
                        "type": "float2"
                    },
                    "vertexCameraOne": {
                        "type": "float3"
                    },
                    "vertexUvOne": {
                        "type": "float2"
                    },
                    "vertexUvTwo": {
                        "type": "float2"
                    },
                    "vertexUvThree": {
                        "type": "float2"
                    },
                    "wrapU": {
                        "type": "int"
                    },
                    "wrapV": {
                        "type": "int"
                    }
                }
            }
        },
        "@texture@.animated": {
            "UV": {
                "attributes": {
                    "coverage": {
                        "type": "float2",
                        "expression": "${frameScale}"
                    },
                    "offset": {
                        "type": "float2",
                        "expression": "${frameId(reverse=true)}"
                    }
                }
            }
        },
        "@texture@.interpolated": {
            "UV": {
                "attributes": {
                    "coverage": {
                        "type": "float2",
                        "expression": "${frameScale}"
                    },
                    "offset": {
                        "type": "float2",
                        "expression": "${frameId(reverse=true)}"
                    }
                }
            },
            "UV2": {
                "type": "JSON:place2dTexture",
                "attributes": {
                    "coverage": {
                        "type": "float2"
                    },
                    "mirrorU": {
                        "type": "int"
                    },
                    "mirrorV": {
                        "type": "int"
                    },
                    "noiseUV": {
                        "type": "float2"
                    },
                    "offset": {
                        "type": "float2"
                    },
                    "repeatUV": {
                        "type": "float2"
                    },
                    "rotateFrame": {
                        "type": "float"
                    },
                    "rotateUV": {
                        "type": "float"
                    },
                    "stagger": {
                        "type": "int"
                    },
                    "translateFrame": {
                        "type": "float2"
                    },
                    "uvCoord": {
                        "type": "float2"
                    },
                    "uvFilterSize": {
                        "type": "float2"
                    },
                    "vertexCameraOne": {
                        "type": "float3"
                    },
                    "vertexUvOne": {
                        "type": "float2"
                    },
                    "vertexUvTwo": {
                        "type": "float2"
                    },
                    "vertexUvThree": {
                        "type": "float2"
                    },
                    "wrapU": {
                        "type": "int"
                    },
                    "wrapV": {
                        "type": "int"
                    },
                    "coverage": {
                        "type": "float2",
                        "expression": "${frameScale}"
                    },
                    "offset": {
                        "type": "float2",
                        "expression": "${frameId(reverse=true,offset=1)}"
                    }
                }
            },
            "FILE2": {
                "type": "JSON:file",
                "attributes": {
                    "outColor": {
                        "type": "float3"
                    },
                    "fileTextureName": {
                        "type": "asset",
                        "value": "@texture@"
                    },
                    "filterType": {
                        "type": "int",
                        "value": 0
                    },
                    "rsFilterEnable": {
                        "type": "int",
                        "value": 0
                    },
                    "coverage": {
                        "type": "float2",
                        "connection": "UV2.coverage"
                    },
                    "mirrorU": {
                        "type": "int",
                        "connection": "UV2.mirrorU"
                    },
                    "mirrorV": {
                        "type": "int",
                        "connection": "UV2.mirrorV"
                    },
                    "noiseUV": {
                        "type": "float2",
                        "connection": "UV2.noiseUV"
                    },
                    "offset": {
                        "type": "float2",
                        "connection": "UV2.offset"
                    },
                    "repeatUV": {
                        "type": "float2",
                        "connection": "UV2.repeatUV"
                    },
                    "rotateFrame": {
                        "type": "float",
                        "connection": "UV2.rotateFrame"
                    },
                    "rotateUV": {
                        "type": "float",
                        "connection": "UV2.rotateUV"
                    },
                    "stagger": {
                        "type": "int",
                        "connection": "UV2.stagger"
                    },
                    "translateFrame": {
                        "type": "float2",
                        "connection": "UV2.translateFrame"
                    },
                    "uvCoord": {
                        "type": "float2",
                        "connection": "UV2.outUV"
                    },
                    "uvFilterSize": {
                        "type": "float2",
                        "connection": "UV2.outUvFilterSize"
                    },
                    "vertexCameraOne": {
                        "type": "float3",
                        "connection": "UV2.vertexCameraOne"
                    },
                    "vertexUvOne": {
                        "type": "float2",
                        "connection": "UV2.vertexUvOne"
                    },
                    "vertexUvTwo": {
                        "type": "float2",
                        "connection": "UV2.vertexUvTwo"
                    },
                    "vertexUvThree": {
                        "type": "float2",
                        "connection": "UV2.vertexUvThree"
                    },
                    "wrapU": {
                        "type": "int",
                        "connection": "UV2.wrapU"
                    },
                    "wrapV": {
                        "type": "int",
                        "connection": "UV2.wrapV"
                    }
                }
            },
            "INTERPOLATE": {
                "type": "JSON:RedshiftColorLayer",
                "attributes": {
                    "output": {
                        "type": "float3"
                    },
                    "layer1_color": {
                        "type": "float3",
                        "connection": "FILE.outColor"
                    },
                    "base_color": {
                        "type": "float3",
                        "connection": "FILE2.outColor"
                    },
                    "layer1_alpha": {
                        "type": "float",
                        "expression": "${interpFactor}"
                    }
                }
            },
            "MAT": {
                "attributes": {
                    "diffuse_color": {
                        "connection": "INTERPOLATE.output"
                    }
                }
            }
        },
        "@texture@.a": {
            "FILE": {
                "attributes": {
                    "outAlpha": {
                        "type": "float"
                    }
                }
            },
            "MAT": {
                "attributes": {
                    "opacity_colorR": {
                        "type": "float",
                        "connection": "FILE.outAlpha"
                    },
                    "opacity_colorG": {
                        "type": "float",
                        "connection": "FILE.outAlpha"
                    },
                    "opacity_colorB": {
                        "type": "float",
                        "connection": "FILE.outAlpha"
                    }
                }
            }
        },
        "@texture@.a&&@texture@.interpolated": {
            "FILE": {
                "attributes": {
                    "outAlpha": {
                        "type": "float"
                    }
                }
            },
            "FILE2": {
                "attributes": {
                    "outAlpha": {
                        "type": "float"
                    }
                }
            },
            "INTERPOLATEALPHA": {
                "type": "JSON:RedshiftColorLayer",
                "attributes": {
                    "output": {
                        "type": "float3"
                    },
                    "color2R": {
                        "type": "float",
                        "connection": "FILE.outAlpha"
                    },
                    "color2G": {
                        "type": "float",
                        "connection": "FILE.outAlpha"
                    },
                    "color2B": {
                        "type": "float",
                        "connection": "FILE.outAlpha"
                    },
                    "color1R": {
                        "type": "float",
                        "connection": "FILE2.outAlpha"
                    },
                    "color1G": {
                        "type": "float",
                        "connection": "FILE2.outAlpha"
                    },
                    "color1B": {
                        "type": "float",
                        "connection": "FILE2.outAlpha"
                    },
                    "blender": {
                        "type": "float",
                        "expression": "${interpFactor}"
                    }
                }
            },
            "MAT": {
                "attributes": {
                    "opacity_color": {
                        "type": "float3",
                        "connection": "INTERPOLATEALPHA.output"
                    }
                }
            }
        },
        "@biomeColor@":{
            "BIOME_MULT": {
                "type": "JSON:RedshiftVertexColor",
                "attributes": {
                    "output": {
                        "type": "float3"
                    },
                    "vertexSet":{
                        "type": "string",
                        "value": "Cd"
                    },
                    "colorGain": {
                        "type": "float3",
                        "connection": "FILE.outColor"
                    }
                }
            },
            "MAT": {
                "attributes": {
                    "diffuse_color": {
                        "type": "float3",
                        "connection": "BIOME_MULT.outColor"
                    }
                }
            }
        }
    }
}